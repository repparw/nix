# Bulk rename
cmd bulk ${{
  /bin/sh -c "vimv $(echo -e "$fx" | xargs -i echo "\\'{}\\'" | xargs echo)"
}}
map b bulk

cmd on-cd &{{
        printf "\033]0; $PWD\007" > /dev/tty;
		zoxide add "$PWD"
    }}
on-cd

map q quit

set previewer ~/.config/lf/lf_kitty_preview
set cleaner ~/.config/lf/lf_kitty_clean
